http {
    server {
    listen 80;
    root /www;
    index index.html index.htm;
    server_name www.mytest.com;
    return 301 https://$host$request_uri;
    }
    server {
        listen                  443 ssl;
        ssl_certificate         /etc/ssl/certs/localhost.crt;
        ssl_certificate_key     /etc/ssl/certs/localhost.key;
        root                    /www;
        index                   index.html index.htm;
    }
}